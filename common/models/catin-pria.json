{
  "name": "CatinPria",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "properties": {
    "districtId": {
      "type": "number",
      "required": true
    },
    "no_ktp": {
      "type": "string",
      "required": true,
      "index": {
        "unique": true
      }
    },
    "url_ktp": {
      "type": "string",
      "required": false
    },
    "nama": {
      "type": "string",
      "required": true
    },
    "tanggal_lahir": {
      "type": "date",
      "required": true
    },
    "agama": {
      "type": "string",
      "required": true
    },
    "pekerjaan": {
      "type": "string",
      "required": true
    },
    "alamat": {
      "type": "string",
      "required": true
    },
    "wali_nikah": {
      "type": "string",
      "required": true
    },
    "status": {
      "type": "string",
      "required": true
    }
  },
  "validations": [],
  "relations": {
    "nikahs": {
      "type": "hasMany",
      "model": "Nikah",
      "foreignKey": "catinpriaId"
    },
    "districts": {
      "type": "belongsTo",
      "model": "Districts",
      "foreignKey": "districtId"
    }
  },
  "acls": [],
  "methods": {}
}
